<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping package="com.xuyuan.hibernate3.model">	<class name="Employee">		<id name="id">			<generator class="native" />		</id>		<property name="name"></property>		<many-to-one name="depart" column="depart_id" />	</class></hibernate-mapping>